{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenApi": {
    "Endpoint": {
      "Name": "Web.Bff.AdminPortal V1"
    },
    "Document": {
      "Description": "Admin Portal Aggregator for Web Clients",
      "Title": "Admin Portal Aggregator for Web Clients",
      "Version": "v1"
    }
  },
  "Identity": {
    "Url": "http://localhost:5223",
    "Audience": "adminportalagg",
    "Scopes": {
      "webshoppingagg": "Admin Portal Aggregator for Web Clients"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "c-short": {
        "ClusterId": "catalog",
        "Match": {
          "Path": "c/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/c" }
        ]
      },
      "c-long": {
        "ClusterId": "catalog",
        "Match": {
          "Path": "catalog-svc/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/catalog-svc" }
        ]
      }
    },
    "Clusters": {
      "product": {
        "Destinations": {
          "destination0": {
            "Address": "https://localhost:44398",
            "Swaggers": [
              {
                "PrefixPath": "/catalog-svc",
                "MetadataPath": "/swagger/v1/swagger.json",
                "Paths": [
                  "/swagger/v1/swagger.json"
                ]
              }
            ]
          }
        }
      }
    }
  },
  "Urls": {
    "Product": "https://localhost:44398"
  },
  "ProductUrlHC": "https://localhost:44398/hc"
}